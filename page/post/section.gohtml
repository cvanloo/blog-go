{{$id := MakeUniqueID .}}
<section id="{{urlquery $id}}">
    {{if .SectionLevel1 }}
    <h2><a href="#{{$id}}">{{Render .Heading}}</a></h2>
    {{end}}
    {{if .SectionLevel2 }}
    <h3><a href="#{{$id}}">{{Render .Heading}}</a></h3>
    {{end}}
    {{range .Content}}
    {{Render .}}
    {{end}}
</section>
